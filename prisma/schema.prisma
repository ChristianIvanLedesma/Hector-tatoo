generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  
}


model busquedaDeJugador {
  id        Int      @id @default(autoincrement())
  titulo    String
  aviso     String
  createdAt DateTime @default(now())
}


model contacto{
  id        Int     @id @default(autoincrement())
  nombre    String
  apellido  String
  correo    String
  telefono  String
  mensaje   String
  createAt  DateTime @default(now())

}